{
        "env": "Pong-v0",
        "agent": "policy_gradient",
        "MAX_EPISODES": 10000,
        "MAX_STEPS": 1000,
        "discount_rate": 0.99,
        "reg_param": 1e-2,
        "init_learning_rate": 1e-4,
        "init_exp_rate": 0.5,
        "anneal_step_exp": 1000,
        "anneal_step_lr": 5000,
        "anneal_base_exp": 0.5,
        "anneal_base_lr": 0.5,
        "min_lr": 1e-6,
        "clip_norm": 5,
        "store_eps": 4,
        "store_size": 100,
        "batch_size": 20,
        "save_step": 5000,
        "save_dir": "weights/pg",
        "log_step": 20,
        "estimator_params": {
            "policy_network": {
                "num_cnn_layers": 5,
                "num_fc_layers": 2,
                "trainable": true
             }
        }
}
