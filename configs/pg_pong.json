{
        "env": "Pong-v0",
        "agent": "policy_gradient",
        "MAX_EPISODES": 10000,
        "MAX_STEPS": 1000,
        "discount_rate": 0.99,
        "reg_param": 1e-2,
        "init_learning_rate": 1e-2,
        "init_exp_rate": 0.5,
        "anneal_step_exp": 1000,
        "anneal_step_lr": 10000,
        "anneal_base_exp": 0.5,
        "anneal_base_lr": 0.5,
        "min_lr": 1e-5,
        "clip_norm": 5,
        "store_eps": 4,
        "store_size": 100,
        "batch_size": 100,
        "save_step": 10000,
        "save_dir": "weights/pg"
}
