{
        "env": "CartPole-v0",
        "agent": "ddpg",
        "MAX_EPISODES": 1000000,
        "MAX_STEPS": 1000,
        "discount_rate": 0.99,
        "tau": 0.01,
        "reg_param": 1e-3,
        "l2": 0.001,
        "init_learning_rate": 1e-4,
        "init_exp_rate": 0.5,
        "anneal_step_exp": 200000,
        "anneal_step_lr": 200000,
        "anneal_base_exp": 0.99,
        "anneal_base_lr": 1,
        "min_lr": 1e-6,
        "min_exp": 0,
        "clip_norm": 5,
        "store_eps": 1,
        "store_size": 50,
        "memory_size": 64,
        "batch_size": 8,
        "save_step": 50000,
        "save_dir": "weights/ac",
        "log_step": 20,
        "estimator_params": {
            "policy_network": {
                "name": "fc", 
                "num_hids": [20],
                "trainable": true
             },
            "value_network": {
                "name": "fc_action", 
                "num_hids": [20],
                "trainable": true
             }
        },
        "inner_state_params": {
            "name": "multi_step_inner_state",
            "num_steps": 1

        }
}
